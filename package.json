{
  "name": "projeto-arkmeds",
  "version": "1.0.0",
  "description": "Teste da arkmeds",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --testPathIgnorePatterns=integration.test",
    "test:all": "jest",
    "test:coverage": "jest --coverage",
    "migrate:up": "node-pg-migrate up -m src/infrastructure/persistence/migrations",
    "migrate:down": "node-pg-migrate down -m src/infrastructure/persistence/migrations",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "docker:logs:worker": "docker compose logs -f worker",
    "worker": "node dist/receipt-worker.js",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/receipt-worker.ts"
  },
  "author": "Lucas Emanuel Silva Dos Santos",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "node-pg-migrate": "^8.0.4",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bullmq": "^5.34.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ioredis": "^5.4.2",
    "pg": "^8.17.1",
    "pino": "^10.2.0"
  }
}
